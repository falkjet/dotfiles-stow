!Xterm
XTerm*faceName: JetBrainsMono Nerd Font
XTerm*font: 7x13
XTerm*faceSize: 10
XTerm.termName: xterm-256color
XTerm*geometry: 106x32
XTerm*metaSendsEscape: true
XTerm*locale: false
XTerm*utf8: true
XTerm*backarrowKey: false
XTerm.ttyModes: erase ^?
XTerm*renderFont: true

XTerm*vt100.translations: #override \
    Shift Ctrl <Key> C: copy-selection(CLIPBOARD) \n\
    Shift Ctrl <Key> V: insert-selection(CLIPBOARD)

!Dracula colors https://draculatheme.com/
*.foreground: #F8F8F2
*.background: #141414
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

!URxvt
URxvt*scrollBar: false
URxvt*font: xft:JetBrainsMono Nerd Font
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0000/c800
